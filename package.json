{
  "name": "electron-app",
  "private": true,
  "version": "0.0.0",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:dev:electron\"",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "vue-tsc -b && vite build && npm run build:electron",
    "build:electron": "electron-builder",
    "preview": "vite preview",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@vicons/ionicons5": "^0.12.0",
    "better-sqlite3": "^12.4.1",
    "drizzle-orm": "^0.36.4",
    "naive-ui": "^2.39.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.24",
    "vue-files-preview": "^1.0.38",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.5.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.30.6",
    "electron": "30",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vue-tsc": "^3.1.3",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "MyVueElectronApp",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}
